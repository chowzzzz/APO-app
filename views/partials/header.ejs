      $("#search_nric").on("keypress", function (e) {
        var value = $(this).val();
        var keypress_value = String.fromCharCode(e.which);
        var patt1 = new RegExp("[A-Za-z]");
        var patt2 = new RegExp("[0-9]");

        if (value.length == 0) { // FIRST LETTER BEING PRESSED
          if (!patt1.test(keypress_value)) {
            e.preventDefault();
          }
        } else if (value.length >= 1 && value.length <= 7) {
          if (!patt2.test(keypress_value)) {
            e.preventDefault();
          }
        } else if (value.length == 8) {
          if (!patt1.test(keypress_value)) {
            e.preventDefault();
          }
        }
      });

      $("#nric_textbox").on("keypress", function (e) {
        var value = $(this).val();
        var keypress_value = String.fromCharCode(e.which);
        var patt1 = new RegExp("[A-Za-z]");
        var patt2 = new RegExp("[0-9]");

        if (value.length == 0) { // FIRST LETTER BEING PRESSED
          if (!patt1.test(keypress_value)) {
            e.preventDefault();
          }
        } else if (value.length >= 1 && value.length <= 7) {
          if (!patt2.test(keypress_value)) {
            e.preventDefault();
          }
        } else if (value.length == 8) {
          if (!patt1.test(keypress_value)) {
            e.preventDefault();
          }
        }
      });

